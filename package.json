{
  "name": "openclaw-proxy",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch index.ts",
    "start": "bun index.ts",
    "test": "bun test",
    "bg": "tmux new -s proxy -d 'bun run start'",
    "attach": "tmux attach -t proxy",
    "stop": "tmux kill-session -t proxy"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "openai": "^6.25.0"
  }
}
